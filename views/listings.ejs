<%- include('header.ejs') %>
<h1>Community Listings</h1>
<nav>
 <a href="/listings/new" class="createButton"> Create New Listing</a>
</nav>

<ul class="listings">
  <% for (let i = 0; i < listings.length; i++) { %>
    <li>
      <a href="/listings/<%=listings[i].id%>">
        <%=listings[i].listingName%></br>
        <img src="<%=listings[i].img%>" class="listingImg"></br>
        <%=listings[i].authorName%></br>
        <%=listings[i].description%></br>
        <%=listings[i].price%></br>
      </a>
    </li>
  <% } %>
</ul>

<%- include('footer.ejs') %>